        deleteThread (id) {\n            store.threads = store.threads.filter(function(obj) {\n              return obj.id !== id;\n            });\n            this.$router.push('/')\n          },\n          goBack() {\n            this.$router.push('/')\n          }\n      }\n    }\n</script>\n\n<style scoped>\n\n.button {\n  margin-right: 20px;\n}\n\n</style>\n"]}]}